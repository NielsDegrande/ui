{
  "name": "ui",
  "version": "0.0.0",
  "description": "A user interface template for React applications.",
  "keywords": [
    "react",
    "template",
    "ui"
  ],
  "author": "Niels Degrande <niels@degran.de>",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write . --ignore-unknown",
    "format:ci": "prettier --check . --ignore-unknown",
    "knip": "knip",
    "lint": "eslint . --fix --report-unused-disable-directives --max-warnings 0",
    "lint:ci": "eslint . --report-unused-disable-directives --max-warnings 0",
    "models": "sta --axios --path http://localhost:8080/api/openapi.json  --output src/api --modular",
    "playwright": "npx --yes playwright test",
    "playwright:ci": "CI=true npx --yes playwright test",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:ci:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*": [
      "pnpm format"
    ],
    "*.{ts, tsx}": [
      "pnpm lint"
    ]
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "5.90.2",
    "@tanstack/react-query-devtools": "5.90.2",
    "axios": "1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "formik": "2.4.6",
    "i18next": "25.5.2",
    "lucide-react": "^0.545.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "6.0.0",
    "react-i18next": "16.0.0",
    "react-router-dom": "7.9.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "yup": "1.7.1",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@playwright/test": "1.56.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.7.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "@vitejs/plugin-react-swc": "4.1.0",
    "@vitest/coverage-v8": "2.1.9",
    "autoprefixer": "^10.4.21",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-no-relative-import-paths": "1.6.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-unused-imports": "4.2.0",
    "globals": "^16.4.0",
    "husky": "9.1.7",
    "jsdom": "27.0.1",
    "knip": "5.64.1",
    "msw": "2.11.5",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "swagger-typescript-api": "13.2.15",
    "tailwindcss": "^3",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vitest": "2.1.9"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0",
      "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5",
      "esbuild@<=0.24.2": ">=0.25.0",
      "micromatch@<4.0.8": ">=4.0.8",
      "nanoid@<3.3.8": ">=3.3.8",
      "path-to-regexp@>=4.0.0 <6.3.0": ">=6.3.0",
      "rollup@>=4.0.0 <4.22.4": ">=4.22.4",
      "smol-toml@<=1.3.0": ">=1.3.1",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "@babel/runtime@<7.26.10": ">=7.26.10",
      "@eslint/plugin-kit@<0.3.3": ">=0.3.3",
      "form-data@>=4.0.0 <4.0.4": ">=4.0.4"
    }
  },
  "packageManager": "pnpm@9.15.0"
}
