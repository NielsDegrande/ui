{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write --ignore-unknown",
    "format:ci": "prettier --check --ignore-unknown",
    "knip": "knip",
    "lint": "eslint . --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:ci": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "models": "sta --axios --path http://localhost:8080/api/openapi.json  --output src/api --modular",
    "playwright": "bunx playwright test",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:ci:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*": [
      "bun run format"
    ],
    "*.{ts, tsx}": [
      "bun run lint"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@mui/icons-material": "5.15.18",
    "@mui/material": "5.15.17",
    "@tanstack/react-query": "5.37.1",
    "@tanstack/react-query-devtools": "5.37.1",
    "axios": "1.7.1",
    "formik": "2.4.6",
    "i18next": "23.11.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-i18next": "14.1.1",
    "react-router-dom": "6.23.1",
    "react-toastify": "10.0.5",
    "yup": "1.4.0",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@playwright/test": "1.44.0",
    "@tanstack/eslint-plugin-query": "5.35.6",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@types/bun": "1.1.3",
    "@types/node": "20.12.12",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-no-relative-import-paths": "1.5.4",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.7",
    "eslint-plugin-unused-imports": "3.2.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "knip": "5.17.0",
    "msw": "2.3.0",
    "prettier": "3.2.5",
    "swagger-typescript-api": "13.0.3",
    "typescript": "5.2.2",
    "vite": "5.2.11",
    "vitest": "1.6.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
