{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write . --ignore-unknown",
    "format:ci": "prettier --check . --ignore-unknown",
    "knip": "knip",
    "lint": "eslint . --fix --report-unused-disable-directives --ignore-pattern 'dist/*' --max-warnings 0",
    "lint:ci": "eslint . --report-unused-disable-directives --ignore-pattern 'dist/*' --max-warnings 0",
    "models": "sta --axios --path http://localhost:8080/api/openapi.json  --output src/api --modular",
    "playwright": "npx --yes playwright test",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:ci:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*": [
      "pnpm format"
    ],
    "*.{ts, tsx}": [
      "pnpm lint"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "5.16.7",
    "@mui/material": "5.16.7",
    "@tanstack/react-query": "5.59.20",
    "@tanstack/react-query-devtools": "5.59.15",
    "axios": "1.7.7",
    "formik": "2.4.6",
    "i18next": "23.15.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.1.2",
    "react-i18next": "15.0.2",
    "react-router-dom": "6.27.0",
    "react-toastify": "10.0.6",
    "yup": "1.4.0",
    "zustand": "5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@playwright/test": "1.48.2",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@types/node": "22.7.7",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react-swc": "3.7.1",
    "@vitest/coverage-v8": "2.0.5",
    "eslint": "9.7.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-no-relative-import-paths": "1.5.5",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "7.35.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "^15.12.0",
    "husky": "9.1.1",
    "jsdom": "25.0.0",
    "knip": "5.32.0",
    "msw": "2.5.2",
    "prettier": "3.3.3",
    "swagger-typescript-api": "13.0.22",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vitest": "2.0.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "pnpm": {
    "overrides": {
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "micromatch@<4.0.8": ">=4.0.8",
      "path-to-regexp@>=4.0.0 <6.3.0": ">=6.3.0",
      "vite@>=5.4.0 <5.4.6": ">=5.4.6",
      "vite@>=5.4.0 <=5.4.5": ">=5.4.6",
      "rollup@>=4.0.0 <4.22.4": ">=4.22.4",
      "cookie@<0.7.0": ">=0.7.0"
    }
  }
}
